{
  "manifest_version": 2,
  "name": "Focus",
  "version": "1.0",
  "description": "Prevents one from being distracted by YouTube videos",
  "web_accessible_resources": [
    "images/aishath-naj-662589-unsplash.jpg"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["jquery-3.4.1.slim.js", "main.js"],
      "css": ["index.css"]
    }
  ]
}